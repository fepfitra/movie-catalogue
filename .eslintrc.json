{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "no-new": "off",
      "no-restricted-globals": "off",
      "no-console": "off",
      "no-undef": "off",
      "consistent-return": "off",
      "no-prototype-builtins": "off",
      "no-return-assign": "off"
    }
}
